#include <string>
#include <iostream>
using namespace std;
int main() {
	int i, l, I = -1;
	string s;
	getline(cin,s);
	l = s.size();
	for (i = 1; i < l - 1; i++)
		if ((s[i] == ' ') && (s[i - 1] != ' ') && (s[i + 1] != ' '))
			if (i - 1 == I) {
				cout << s[i + 1] << " ";
				I = i + 1;
			}
			else {
				cout << s[i - 1] << " " << s[i + 1] << " ";
				I = i + 1;
			}
	return 0;
}
